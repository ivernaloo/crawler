{
  "name": "simple-webcrawler-javascript",
  "version": "0.0.0",
  "description": "A simple webcrawler written in JavaScript",
  "main": "crawler.js",
  "author": "aloo",
  "license": "MIT",
  "scripts": {
    "crawler": "nodemon crawler/app.js --verbose",
    "web": "nodemon app.js --verbose",
    "deduplicate": "nodemon build/leancloud.js --exec babel-node --presets es2015,stage-3",
    "build": "babel -w data -d build",
    "debug": "concurrently --kill-others 'npm run build' 'npm run deduplicate'"
  },
  "dependencies": {
    "babel-core": "^6.13.2",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-stage-3": "^6.11.0",
    "cheerio": "^0.19.0",
    "ejs": "^2.5.1",
    "express": "^4.14.0",
    "moment": "^2.14.1",
    "request": "^2.65.0",
    "url-parse": "^1.0.5"
  },
  "devDependencies": {
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.12.0",
    "concurrently": "^2.2.0"
  },
  "babel": {
    "plugins": [
      "transform-async-to-generator"
    ]
  }
}
