<!DOCTYPE HTML>
<head>
<link href="//cdn.bootcss.com/bootstrap/3.3.7/css/bootstrap.min.css" rel="stylesheet">
<link rel="stylesheet" href="../web/css/index.css">
<title>教程资料</title>
</head>
<body>
<nav class="navbar navbar-default navbar-fixed-top" role="navigation">
    <div class="container-fluid">
        <div class="navbar-left panel-body">
            <span class="label label-default">react</span>
            <span class="label label-default">angular</span>
            <span class="label label-default">node</span>
            <span class="label label-default">android</span>
            <span class="label label-default">augmented reality</span>
        </div>
        <div class="navbar-right">
            <form class="navbar-form navbar-left" role="search">
                <div class="form-group">
                    <input type="text" class="form-control" placeholder="Search" name="query" v-model="searchQuery">
                </div>
                <button type="submit" class="btn btn-default">搜索</button>
            </form>
        </div>
    </div><!-- /.container-fluid -->
</nav>
<ul class="list-group">
    <% List.forEach(function (item) { %>
    <li class="list-group-item">
        <span class="badge"><%= moment(item.createdAt).format('YYYY / MM / DD') %></span>
        <a href="<%= item.url %>"><%= item.name %></a>
    </li>
    <% }) %>
</ul>


<!-- component template -->
<script type="text/x-template" id="list-template">
    <ul class="list-group" v-for="item in data">
        <li class="list-group-item">
            <span class="badge"></span>
            <a href={{item[url]}}>{{item[name]}}</a>
        </li>
    </ul>
</script>



<script src="//cdn.bootcss.com/jquery/3.1.0/jquery.min.js"></script>
<script src="/web/js/lib/vue.min.js"></script>
<script src="../web/js/index.js"></script>
</body>
